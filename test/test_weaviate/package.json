{
    "name": "weaviate-product-manager",
    "version": "1.0.0",
    "description": "Simple TypeScript project to manage Weaviate schema and products",
    "main": "index.ts",
    "scripts": {
        "start": "ts-node src/index.ts",
        "deleteindex:product": "ts-node src/deleteIndexProduct.ts",
        "create:product": "ts-node src/createProduct.ts",
        "get:product": "ts-node src/findProduct.ts",
        "search": "ts-node src/search.ts",
        "build": "tsc",
        "start:prod": "node dist/index.js",
        "lint": "npx eslint ./src --ext .ts",
        "format": "npx prettier --write ./src",
        "type-check": "tsc --noEmit"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "axios": "^1.9.0",
        "weaviate-ts-client": "^2.2.0"
    },
    "devDependencies": {
        "@types/node": "^22.15.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
